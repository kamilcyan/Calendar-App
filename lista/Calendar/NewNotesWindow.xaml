<Window x:Class="Calendar.NewNotesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calendar"
        mc:Ignorable="d"
        Title="NewNotesWindow" Height="450" Width="300">
    <Grid>
        
              <StackPanel Margin="10">

                        <TextBlock x:Name="DateShow" Margin="0,0,0,5"
                                   />
                        <TextBox x:Name="InsertNote"
                                 Text="Write note here" Margin="0,0,0,5"/>
                        <Button x:Name="DoneButton" Margin="0,0,0,5"
                                Content="Done"
                                Click="DoneButton_Click"
                                />
                        <Button x:Name="CancelButton" Margin="0,0,0,5"
                                Content="Cancel"
                                Click="CancelButton_Click"
                                />
            <TextBlock x:Name="ShowNotes" Margin="0,0,0,5"
                        />

        </StackPanel>
               

    </Grid>
</Window>
